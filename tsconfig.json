{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": false,
    "paths": {
      "@screens/*": ["./src/screens/*"],
      "@core/*": ["./src/core/*"],
      "@components/*": ["./src/components/*"],
      "@assets/*": ["./assets/*"],
      "@services/*": ["./src/services/*"],
      "@shared/*": ["./src/shared/*"],
      "@/*": ["./src/*"],
    }
  }
}
